## Copy this file to .env and fill values before running scripts
# Do NOT commit your real credentials to version control.

# Auvo API credentials
AUVO_API_KEY=your_auvo_api_key_here
AUVO_API_TOKEN=your_auvo_api_token_here

# Postgres connection (used by run_migration.py, auvo_sync.py, web_ui.py)
PGHOST=localhost
PGPORT=5432
PGUSER=auvo
PGPASSWORD=auvo_pass
PGDATABASE=auvo

